{
  "name": "susee",
  "version": "0.1.0",
  "description": "Tiny TypeScript Bundler",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    },
    "./banner-text": {
      "import": {
        "default": "./dist/banner-text/index.mjs",
        "types": "./dist/banner-text/index.d.mts"
      },
      "require": {
        "default": "./dist/banner-text/index.cjs",
        "types": "./dist/banner-text/index.d.cts"
      }
    },
    "./minify": {
      "import": {
        "default": "./dist/minify/index.mjs",
        "types": "./dist/minify/index.d.mts"
      },
      "require": {
        "default": "./dist/minify/index.cjs",
        "types": "./dist/minify/index.d.cts"
      }
    }
  },
  "scripts": {
    "build": "tsx build.ts"
  },
  "keywords": [
    "bundler"
  ],
  "author": {
    "name": "Pho Thin Mg",
    "email": "phothinmg@disroot.org",
    "url": "https://phothinmg.github.io/"
  },
  "license": "ISC",
  "dependencies": {
    "dependensia": "^0.0.7",
    "terser": "^5.46.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.0.10",
    "tsx": "^4.21.0"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist/**/*"
  ]
}
